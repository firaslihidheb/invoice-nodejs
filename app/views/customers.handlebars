<div class="container">
  <div class="col-md-6">
    <h2>Clients</h2>
  </div>
  <div class="col-md-6">
    <div class="add_more_button add_new_customer" style="width: 124px"><i
        class="fa fa-plus"
        aria-hidden="true"
      ></i>
      Add new Client
    </div>
    <div
      class="remove_more_button remove_new_customer"
      style="width: 124px; display: none"
    ><i class="fa fa-times" aria-hidden="true"></i>
      Cancel
    </div>
  </div>
</div>
<div class="container">
  <form class="add-customer-form" style="display: none">
    <hr />
    <h4 class="color-green">Nouveau client</h4>
    <div class="row m-t-15">
      <div class="col-md-6">
        <div class="form-group">
          <label>Raison Social</label>
          <input
            type="text"
            class="form-control"
            placeholder="Enter Raison Social"
             name="raison_social"
            required
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label>Client Number</label>
          <input
            type="number"
            step="1"
            minlength="10"
            min="1000"
            name="client_number"
            id="client_number"
            tabindex="1"
            class="form-control"
            placeholder="Enter Client Number"
            required
            title="10 digit Numbers"
          />
        </div>
      </div>
      <div class="row m-t-15">
        <div class="col-md-6">
          <div class="form-group">
            <label>Email</label>
            <input
              type="email"
              name="email"
              id="email"
              tabindex="1"
              class="form-control"
              placeholder="Email Address"
              required
            />
          </div>
        </div>
      </div>

      <div class="col-md-6">
        <div class="form-group">
          <label>Interlocuteur</label>
          <input
            type="text"
            name="interlocuteur"
            id="interlocuteur"
            tabindex="1"
            class="form-control"
            placeholder="Enter Interlocuteur"
            required
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label>Email Interlocuteur</label>
          <input
            type="email"
            name="email_interlocuteur"
            id="email_interlocuteur"
            tabindex="1"
            class="form-control"
            placeholder="Enter Email Interlocuteur"
            required
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label>Numero et Voie</label>
          <input
            type="text"
            name="NumeroEtVoie"
            id="NumeroEtVoie"
            tabindex="1"
            class="form-control"
            placeholder="Enter Numero et Voie"
            required
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label>Code Postal</label>
          <input
            type="text"
            name="CodePostal"
            id="CodePostal"
            tabindex="1"
            class="form-control"
            placeholder="Enter Code Postal"
            required
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label>Commune</label>
          <input
            type="text"
            name="Commune"
            id="Commune"
            tabindex="1"
            class="form-control"
            placeholder="Enter Commune"
            required
          />
        </div>
      </div>
     

      <div class="col-md-6">
        <div class="form-group">
          <label>TIN No</label>
          <input
            type="number"
            class="form-control"
            placeholder="Enter TIN No"
            name="tin_no"
            required
          />
        </div>
      </div>
    </div>
    <div class="customer_alert_div alert" style="display: none"></div>
    <button type="submit" class="btn submit-btn">Submit</button>
  </form>
  <hr />
  <table id="customers-table" class="display:none" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>Raison Social</th>
        <th>client_number</th>
        <th>Email</th>
        <th>Interlocuteur</th>
        <th>Email interlocuteur</th>
        <th>Numero Et Voie</th>
        <th>Code Postal</th>
        <th>Commune</th>
        <th>TIN No</th>
      </tr>
    </thead>
  </table>
  <div class="no_data_found" style=""></div>
</div>
